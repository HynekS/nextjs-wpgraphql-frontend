    "@emotion/styled": "^11.3.0",
    "dedent": "^0.7.0",
    "eslint-config-next": "^12.0.3",
    "graphql": "^15.5.0",
    "next": "^10.0.0",
    "nextjs-breadcrumbs": "^0.9.9",
    "normalize.css": "^8.0.1",
    "open-color": "^1.8.0",
    "preact": "^10.5.13",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/introspection": "1.18.1",
