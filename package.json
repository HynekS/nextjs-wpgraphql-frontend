    "eslint-plugin-react": "^7.27.0",
    "fs": "0.0.1-security",
    "fs-extra": "^9.1.0",
    "isomorphic-fetch": "^3.0.0",
    "slugify": "^1.5.0",
    "tailwindcss": "^2.1.1",
    "ts-node": "^9.1.1",
    "twin.macro": "^2.3.3",
    "typescript": "^4.2.4"
